{
  "name": "snap-css-vsc",
  "displayName": "Snap CSS",
  "publisher": "SnapCSS",
  "icon": "images/logo.png",
  "repository": "https://github.com/AAiT-Final-Project/SnapCSS/tree/main/snap-css-vsc",
  "description": "A Visual Studio Code extension for the SnapCSS CSS Optimization tool.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:snap-css-vsc.optimize"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snap-css-vsc.optimize",
        "category": "Snap CSS",
        "title": "Optimize CSS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "snappy-css": "^0.2.8"
  }
}
